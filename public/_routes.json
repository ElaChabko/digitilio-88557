{
  "version": 1,
  "include": ["/*"],
  "exclude": [
    "/assets/*",
    "/og/*",
    "/share/*",
    "/favicon.ico",
    "/favicon.png",
    "/robots.txt",
    "/sitemap.xml"
  ],
  "routes": [
    {
      "src": "^/(?!assets/|og/|share/|favicon\\.ico$|favicon\\.png$|robots\\.txt$|sitemap\\.xml$).*",
      "dest": "/index.html"
    }
  ]
}
